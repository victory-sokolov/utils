{
    "name": "@vsokolov/utils",
    "version": "0.12.0",
    "license": "MIT",
    "private": false,
    "type": "module",
    "description": "Various TypeScript/JavaScripts utils functions for node and browser",
    "author": {
        "email": "viktorsokolov.and@gmail.com",
        "name": "Viktor Sokolov"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/victory-sokolov/utils.git"
    },
    "bugs": {
        "url": "https://github.com/victory-sokolov/utils/issues"
    },
    "keywords": [
        "utils",
        "typescript utils",
        "javascript utils"
    ],
    "main": "dist/index.cjs",
    "module": "dist/index.esm.js",
    "typings": "dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.esm.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "rollup -c",
        "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js tests/ --runInBand",
        "coverage": "jest --coverage",
        "lint": "eslint .",
        "docs": "yarn run typedoc ./src --out docs",
        "format": "prettier --write 'src/**/*.ts'",
        "prepublishOnly": "yarn run lint && yarn test",
        "preversion": "yarn run lint && yarn test",
        "version": "yarn run format && git add -A src",
        "postversion": "git push && git push --tags",
        "typecheck": "tsc --noEmit",
        "clean": "rimraf ./node_modules && rimraf dist",
        "commit": "cz",
        "semantic-release": "semantic-release",
        "prepare": "husky install",
        "prepush": "git pull --rebase"
    },
    "devDependencies": {
        "@rollup/plugin-alias": "^4.0.2",
        "@rollup/plugin-commonjs": "^23.0.3",
        "@rollup/plugin-json": "^5.0.2",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^9.0.2",
        "@semantic-release/github": "^8.0.7",
        "@semantic-release/npm": "^10.0.5",
        "@semantic-release/release-notes-generator": "^10.0.3",
        "@types/jest": "^29.2.6",
        "@types/node": "^18.11.18",
        "@types/webrtc": "^0.0.33",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.32.0",
        "eslint-plugin-jest": "^27.2.1",
        "esno": "^0.16.3",
        "husky": "^8.0.3",
        "jest": "^29.3.1",
        "jest-environment-jsdom": "^29.3.1",
        "prettier": "^2.8.2",
        "rollup": "^3.5.0",
        "rollup-plugin-dts": "^5.0.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "semantic-release": "^19.0.5",
        "source-map": "^0.7.4",
        "ts-jest": "^29.0.5",
        "typedoc": "^0.23.23",
        "typedoc-plugin-missing-exports": "^1.0.0",
        "typescript": "4.9.4",
        "user-agent-data-types": "^0.3.0"
    },
    "jest": {
        "preset": "ts-jest/presets/default-esm",
        "collectCoverage": true,
        "testEnvironment": "jsdom",
        "collectCoverageFrom": [
            "./src/**/{!(index|file),}.ts"
        ],
        "testPathIgnorePatterns": [
            "tests/node/*"
        ]
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run prepush"
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
