{
    "name": "@vsokolov/utils",
    "version": "0.12.0",
    "license": "MIT",
    "private": false,
    "type": "module",
    "description": "Various TypeScript/JavaScripts utils functions for node and browser",
    "author": {
        "email": "viktorsokolov.and@gmail.com",
        "name": "Viktor Sokolov"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/victory-sokolov/utils.git"
    },
    "bugs": {
        "url": "https://github.com/victory-sokolov/utils/issues"
    },
    "keywords": [
        "utils",
        "typescript utils",
        "javascript utils",
        "nodejs utils"
    ],
    "sideEffects": false,
    "main": "dist/index.cjs",
    "module": "dist/index.esm.js",
    "typings": "dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.esm.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist/*"
    ],
    "scripts": {
        "build": "rollup -c",
        "test": "jest tests/ --runInBand",
        "coverage": "jest --coverage",
        "lint": "eslint .",
        "docs": "yarn run typedoc ./src --out docs",
        "format": "prettier --write 'src/**/*.ts'",
        "prepublishOnly": "yarn run lint && yarn test",
        "preversion": "yarn run lint && yarn test",
        "version": "yarn run format && git add -A src",
        "postversion": "git push && git push --tags",
        "typecheck": "tsc --noEmit",
        "clean": "rimraf ./node_modules && rimraf dist",
        "commit": "cz",
        "semantic-release": "semantic-release",
        "prepare": "husky install",
        "prepush": "git pull --rebase"
    },
    "devDependencies": {
        "@babel/core": "^7.22.19",
        "@babel/preset-env": "^7.22.15",
        "@rollup/plugin-alias": "5.0.0",
        "@rollup/plugin-commonjs": "25.0.4",
        "@rollup/plugin-json": "6.0.0",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^10.0.4",
        "@semantic-release/github": "^9.0.6",
        "@semantic-release/npm": "^10.0.6",
        "@semantic-release/release-notes-generator": "^11.0.7",
        "@types/jest": "^29.5.4",
        "@types/node": "^20.6.1",
        "@types/webrtc": "0.0.36",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "babel-jest": "^29.7.0",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.49.0",
        "eslint-plugin-jest": "27.2.3",
        "esno": "0.17.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^3.0.3",
        "rollup": "^3.29.1",
        "rollup-plugin-dts": "^6.0.2",
        "rollup-plugin-esbuild": "^5.0.0",
        "semantic-release": "^21.1.1",
        "source-map": "^0.7.4",
        "ts-jest": "^29.0.5",
        "tslib": "^2.6.2",
        "typedoc": "^0.25.1",
        "typedoc-plugin-missing-exports": "^2.1.0",
        "typescript": "^5.2.2",
        "user-agent-data-types": "^0.4.2"
    },
    "jest": {
        "preset": "ts-jest",
        "collectCoverage": true,
        "testEnvironment": "jsdom",
        "collectCoverageFrom": [
            "./src/**/{!(index|file),}.ts"
        ],
        "testPathIgnorePatterns": [
            "tests/node/*"
        ]
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    },
                    "modules": "commonjs"
                }
            ]
        ]
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run prepush"
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "npm": "please-use-yarn",
        "yarn": ">= 1.22.19"
    }
}
